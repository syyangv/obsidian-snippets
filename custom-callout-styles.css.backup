/* ========================================
   CREATIVE CALLOUTS
   ======================================== */

/* Idea Callout - Yellow with lightbulb */
.callout[data-callout="idea"] {
  --callout-color: 255, 193, 7; /* Yellow */
  --callout-icon: <svg xmlns="http://www.w3.org/2000/svg" height="24px" viewBox="0 -960 960 960" width="24px" fill="currentColor"><path d="M480-80q-34 0-57.5-23.5T399-161h162q0 34-23.5 57.5T480-80ZM318-223v-60h324v60H318Zm5-121q-66-43-104.5-107.5T180-580q0-125 87.5-212.5T480-880q125 0 212.5 87.5T780-580q0 64-38 128.5T637-344H323Zm22-60h270q44-26 69.5-71T710-580q0-92-64-156t-156-64q-92 0-156 64t-64 156q0 50 25.5 95t69.5 71Zm135 0Z"/></svg>;
  background: radial-gradient(circle at top left, rgba(var(--callout-color), 0.15), rgba(var(--callout-color), 0.05));
  border-radius: 15px;
  border: 2px solid rgba(var(--callout-color), 0.4);
  box-shadow: 0 4px 12px rgba(var(--callout-color), 0.1);
}

/* Inspiration Callout - Pink with star */
.callout[data-callout="inspiration"] {
  --callout-color: 214, 51, 132; /* Pink */
  --callout-icon: <svg xmlns="http://www.w3.org/2000/svg" height="24px" viewBox="0 -960 960 960" width="24px" fill="currentColor"><path d="m233-120 65-281L80-590l288-25 112-265 112 265 288 25-218 189 65 281-247-149-247 149Z"/></svg>;
  background: linear-gradient(135deg, rgba(var(--callout-color), 0.12), rgba(var(--callout-color), 0.04));
  border-radius: 20px;
  border: 3px solid transparent;
  background-clip: padding-box;
  position: relative;
}

.callout[data-callout="inspiration"]::before {
  content: '';
  position: absolute;
  top: 0; right: 0; bottom: 0; left: 0;
  z-index: -1;
  margin: -3px;
  border-radius: inherit;
  background: linear-gradient(135deg, rgba(var(--callout-color), 0.4), rgba(var(--callout-color), 0.1));
}

/* ========================================
   TECHNICAL CALLOUTS
   ======================================== */

/* Code Callout - Dark with code icon */
.callout[data-callout="code"] {
  --callout-color: 108, 117, 125; /* Gray */
  --callout-icon: <svg xmlns="http://www.w3.org/2000/svg" height="24px" viewBox="0 -960 960 960" width="24px" fill="currentColor"><path d="M320-242 80-482l242-242 56 56-186 186 186 186-56 56Zm320 0-56-56 186-186-186-186 56-56 242 242-242 242Z"/></svg>;
  background-color: rgba(0, 0, 0, 0.05);
  border: 1px solid rgba(var(--callout-color), 0.3);
  border-radius: 6px;
  font-family: 'JetBrains Mono', 'Fira Code', 'Consolas', monospace;
}

.callout[data-callout="code"] .callout-content {
  font-size: 0.9em;
  line-height: 1.5;
}

/* Terminal Callout - Green on dark */
.callout[data-callout="terminal"] {
  --callout-color: 25, 135, 84; /* Dark green */
  --callout-icon: <svg xmlns="http://www.w3.org/2000/svg" height="24px" viewBox="0 -960 960 960" width="24px" fill="currentColor"><path d="M160-160q-33 0-56.5-23.5T80-240v-480q0-33 23.5-56.5T160-800h640q33 0 56.5 23.5T880-720v480q0 33-23.5 56.5T800-160H160Zm0-80h640v-400H160v400Zm140-40-56-56 103-104-104-104 57-56 160 160-160 160Zm180 0v-80h240v80H480Z"/></svg>;
  background-color: #1a1a1a;
  color: #00ff00;
  border: 1px solid rgba(var(--callout-color), 0.5);
  border-radius: 8px;
  font-family: 'JetBrains Mono', 'Courier New', monospace;
}

.callout[data-callout="terminal"] .callout-title {
  background-color: #2a2a2a;
  color: #00ff00;
  margin: -16px -16px 16px -16px;
  padding: 8px 16px;
  border-radius: 8px 8px 0 0;
}

/* ========================================
   STATUS CALLOUTS
   ======================================== */

/* Complete Callout - Green checkmark */
.callout[data-callout="complete"] {
  --callout-color: 25, 135, 84; /* Success green */
  --callout-icon: <svg xmlns="http://www.w3.org/2000/svg" height="24px" viewBox="0 -960 960 960" width="24px" fill="currentColor"><path d="M421-298 274-445l43-43 104 104 227-227 43 43-270 270Zm59 218q-82 0-155-31.5t-127.5-86Q143-252 111.5-325T80-480q0-83 31.5-156t86-127Q252-817 325-848.5T480-880q83 0 156 31.5T763-763q54 54 85.5 127T880-480q0 82-31.5 155T763-197.5q-54 54.5-127 86T480-80Zm0-60q142 0 241-99.5T820-480q0-142-99-241t-241-99q-141 0-240.5 99T140-480q0 141 99.5 240.5T480-140Zm0-340Z"/></svg>;
  background: linear-gradient(135deg, rgba(var(--callout-color), 0.1), rgba(var(--callout-color), 0.05));
  border: 2px solid rgba(var(--callout-color), 0.3);
  border-radius: 10px;
}

/* Pending Callout - Orange clock */
.callout[data-callout="pending"] {
  --callout-color: 253, 126, 20; /* Warning orange */
  --callout-icon: <svg xmlns="http://www.w3.org/2000/svg" height="24px" viewBox="0 -960 960 960" width="24px" fill="currentColor"><path d="m612-292 56-56-148-148v-184h-80v216l172 172ZM480-80q-83 0-156-31.5T197-197q-54-54-85.5-127T80-480q0-83 31.5-156T197-763q54-54 127-85.5T480-880q83 0 156 31.5T763-763q54 54 85.5 127T880-480q0 83-31.5 156T763-197q-54 54-127 85.5T480-80Zm0-80q134 0 227-93t93-227q0-134-93-227t-227-93q-134 0-227 93t-93 227q0 134 93 227t227 93Zm0-320Z"/></svg>;
  background: repeating-linear-gradient(
    90deg,
    rgba(var(--callout-color), 0.05),
    rgba(var(--callout-color), 0.05) 5px,
    rgba(var(--callout-color), 0.1) 5px,
    rgba(var(--callout-color), 0.1) 10px
  );
  border-left: 4px solid rgba(var(--callout-color), 0.8);
  animation: pulse-glow 2s ease-in-out infinite alternate;
}

@keyframes pulse-glow {
  from { box-shadow: 0 0 5px rgba(253, 126, 20, 0.3); }
  to { box-shadow: 0 0 15px rgba(253, 126, 20, 0.1); }
}

/* Blocked Callout - Red with X */
.callout[data-callout="blocked"] {
  --callout-color: 220, 53, 69; /* Danger red */
  --callout-icon: <svg xmlns="http://www.w3.org/2000/svg" height="24px" viewBox="0 -960 960 960" width="24px" fill="currentColor"><path d="M480-80q-83 0-156-31.5T197-197q-54-54-85.5-127T80-480q0-83 31.5-156T197-763q54-54 127-85.5T480-880q83 0 156 31.5T763-763q54 54 85.5 127T880-480q0 83-31.5 156T763-197q-54 54-127 85.5T480-80ZM274-274l206-206 206 206q27-31 40.5-69.5T740-480q0-134-93-227t-227-93q-134 0-227 93t-93 227q0 39 13.5 77.5T274-274Z"/></svg>;
  background-color: rgba(var(--callout-color), 0.08);
  border: 2px solid rgba(var(--callout-color), 0.4);
  border-radius: 8px;
  position: relative;
  opacity: 0.9;
}

/* ========================================
   SPECIAL EFFECT CALLOUTS
   ======================================== */

/* Highlight Callout - Glowing border */
.callout[data-callout="highlight"] {
  --callout-color: 102, 16, 242; /* Purple */
  --callout-icon: <svg xmlns="http://www.w3.org/2000/svg" height="24px" viewBox="0 -960 960 960" width="24px" fill="currentColor"><path d="m233-120 65-281L80-590l288-25 112-265 112 265 288 25-218 189 65 281-247-149-247 149Z"/></svg>;
  background: linear-gradient(135deg, rgba(var(--callout-color), 0.1), rgba(var(--callout-color), 0.05));
  border: 3px solid transparent;
  border-radius: 15px;
  background-clip: padding-box;
  box-shadow: 
    0 0 0 2px rgba(var(--callout-color), 0.3),
    0 8px 16px rgba(var(--callout-color), 0.1);
  animation: highlight-glow 3s ease-in-out infinite;
}

@keyframes highlight-glow {
  0%, 100% { 
    box-shadow: 
      0 0 0 2px rgba(102, 16, 242, 0.3),
      0 8px 16px rgba(102, 16, 242, 0.1);
  }
  50% { 
    box-shadow: 
      0 0 0 3px rgba(102, 16, 242, 0.5),
      0 12px 24px rgba(102, 16, 242, 0.2);
  }
}

/* Bookmark Callout - Folded corner effect */
.callout[data-callout="bookmark"] {
  --callout-color: 220, 53, 69; /* Red */
  --callout-icon: <svg xmlns="http://www.w3.org/2000/svg" height="24px" viewBox="0 -960 960 960" width="24px" fill="currentColor"><path d="M200-120v-640q0-33 23.5-56.5T280-840h400q33 0 56.5 23.5T760-760v640L480-240 200-120Zm80-122 200-86 200 86v-518H280v518Zm0-518h400-400Z"/></svg>;
  background-color: rgba(var(--callout-color), 0.08);
  border-radius: 8px;
  position: relative;
  border: 1px solid rgba(var(--callout-color), 0.2);
}

.callout[data-callout="bookmark"]::before {
  content: '';
  position: absolute;
  top: 0;
  right: 0;
  width: 20px;
  height: 20px;
  background: linear-gradient(-45deg, transparent 50%, rgba(var(--callout-color), 0.3) 50%);
  border-radius: 0 8px 0 0;
}

/* ========================================
   RESPONSIVE ADJUSTMENTS
   ======================================== */

@media (max-width: 768px) {
  .callout[data-callout="terminal"] .callout-title {
    font-size: 0.9em;
  }
  
  .callout[data-callout="code"] .callout-content {
    font-size: 0.8em;
  }
  
  /* Reduce animation intensity on mobile */
  .callout[data-callout="highlight"] {
    animation: none;
    box-shadow: 0 0 0 2px rgba(var(--callout-color), 0.3);
  }
  
  .callout[data-callout="pending"] {
    animation: none;
  }
}

/* ========================================
   DARK THEME ADJUSTMENTS
   ======================================== */

.theme-dark .callout[data-callout="terminal"] {
  background-color: #0d1117;
  color: #00d562;
}

.theme-dark .callout[data-callout="terminal"] .callout-title {
  background-color: #161b22;
  color: #00d562;
}

.theme-dark .callout[data-callout="code"] {
  background-color: rgba(255, 255, 255, 0.03);
}