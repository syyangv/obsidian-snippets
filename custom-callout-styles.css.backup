/* ========================================
   PRODUCTIVITY CALLOUTS
   ======================================== */

/* Priority Callout - Red urgent styling */
.callout[data-callout="priority"] {
  --callout-color: 220, 53, 69; /* Bootstrap danger red */
  --callout-icon: <svg xmlns="http://www.w3.org/2000/svg" height="24px" viewBox="0 -960 960 960" width="24px" fill="currentColor"><path d="M480-120q-33 0-56.5-23.5T400-200q0-33 23.5-56.5T480-280q33 0 56.5 23.5T560-200q0 33-23.5 56.5T480-120Zm-40-240v-480h80v480h-80Z"/></svg>;
  border-left: 4px solid rgb(var(--callout-color)) !important;
  background-color: rgba(var(--callout-color), 0.1);
}

/* Progress Callout - Green with progress bar styling */
.callout[data-callout="progress"] {
  --callout-color: 40, 167, 69; /* Bootstrap success green */
  --callout-icon: <svg xmlns="http://www.w3.org/2000/svg" height="24px" viewBox="0 -960 960 960" width="24px" fill="currentColor"><path d="M200-120q-33 0-56.5-23.5T120-200v-560q0-33 23.5-56.5T200-840h560q33 0 56.5 23.5T840-760v560q0 33-23.5 56.5T760-120H200Zm0-80h560v-560H200v560Zm80-400v-80h400v80H280Zm0 160v-80h280v80H280Zm0 160v-80h400v80H280Z"/></svg>;
  border-left: 4px solid rgb(var(--callout-color)) !important;
  background: linear-gradient(90deg, rgba(var(--callout-color), 0.15) 0%, rgba(var(--callout-color), 0.05) 100%);
}

